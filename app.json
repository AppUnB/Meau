{
  "expo": {
    "name": "my-app",
    "slug": "meau",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/Meau_Icone.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dappmeau.meau"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.dappmeau.meau",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./src/assets/images/Meau_Icone.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "O app precisa de permiss√£o para acessar suas fotos"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "992c0279-203c-479c-af8f-bfbf1e0210c5"
      }
    },
    "owner": "dappmeau"
  }
}